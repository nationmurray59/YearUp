
public class LargestNumber {

	public static void main(String[] args)
	{
		int i = 10;
		
		System.out.println("While loop count down");
		
		while (i > 0)
		{
			System.out.println(l);
			
			i = i - 1;
			
		}
		
		System.out.println("For loop count down");
		
		for(int l = 10; i > 0; l--)
		{
			if (1 ==5)
